{
  "ConnectionStrings": {
    "MongoDb": {
      "Host": "localhost",
      "Port": 27017,
      "UseSSL": false,
      "DbName": "mynchain"
    }
  },
  "Authentication": {
    "DefaultScheme": "Bearer",
    "AuthenticationScheme": "Bearer",
    "Authority": "https://mynidentity.myntech.io",
    "RequireHttpsMetadata": true,
    "Audience": "mynchain_api"
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
